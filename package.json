{
  "name": "@tybys/emnapi",
  "main": "cgen.config.js",
  "scripts": {
    "configure": "cgen configure -e -d",
    "build": "tsc -p ./lib/runtime/tsconfig.json && tsc && node ./script/build.js && cgen build -d",
    "clean": "cgen clean",
    "rebuild": "tsc -p ./lib/runtime/tsconfig.json && tsc && node ./script/build.js && cgen rebuild -e -d",
    "rebuild:r": "tsc -p ./lib/runtime/tsconfig.json && tsc && node ./script/build.js && cgen rebuild -e",
    "build:runtime": "tsc -p ./lib/runtime/tsconfig.json",
    "lint": "eslint lib",
    "test": "jest --runInBand",
    "test:r": "cross-env NODE_ENV=production jest --runInBand"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.25.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.2.4"
  }
}
